{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Protekt Auth",
  "colors": {
    "primary": "#982C29",
    "light": "#DE797D",
    "dark": "#982C29"
  },
  "favicon": "/fav.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "docs/introduction",
              "docs/getting-started/quickstart"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "docs/features/login"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "openapi": "openapi.yaml"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://protekt.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://protekt.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/logo-protekt.svg",
    "dark": "/logo/logo-protekt.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@protekt.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.protekt.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/protekt",
      "github": "https://github.com/protekt",
      "linkedin": "https://linkedin.com/company/protekt"
    }
  }
}
